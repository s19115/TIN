<% targetId; loggedUser %>
<ul class="list-actions">
    <li><a href="/<%= url %>/details/<%= targetId %>" class="list-actions-button-details">Details</a></li>
    <% if(loggedUser) { %>
    <li><a href="/<%= url %>/edit/<%= targetId %>" class="list-actions-button-edit">Edit</a></li>
    <li><a href="/<%= url %>/delete/<%= targetId %>" class="list-actions-button-delete">Delete</a></li>
    <% } %>
</ul>